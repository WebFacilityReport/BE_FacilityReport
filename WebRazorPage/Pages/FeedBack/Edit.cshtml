@page
@model WebRazorPage.Pages.FeedBack.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Feedback</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Feedback.FeedBackId" />
            <div class="form-group">
                <label asp-for="Feedback.CreatedAt" class="control-label"></label>
                <input asp-for="Feedback.CreatedAt" class="form-control" />
                <span asp-validation-for="Feedback.CreatedAt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback.Status" class="control-label"></label>
                <input asp-for="Feedback.Status" class="form-control" />
                <span asp-validation-for="Feedback.Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback.Comment" class="control-label"></label>
                <input asp-for="Feedback.Comment" class="form-control" />
                <span asp-validation-for="Feedback.Comment" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback.NumberFeedBack" class="control-label"></label>
                <input asp-for="Feedback.NumberFeedBack" class="form-control" />
                <span asp-validation-for="Feedback.NumberFeedBack" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback.AccountId" class="control-label"></label>
                <select asp-for="Feedback.AccountId" class="form-control" asp-items="ViewBag.AccountId"></select>
                <span asp-validation-for="Feedback.AccountId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback.EquipmentId" class="control-label"></label>
                <select asp-for="Feedback.EquipmentId" class="form-control" asp-items="ViewBag.EquipmentId"></select>
                <span asp-validation-for="Feedback.EquipmentId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
